@model IEnumerable<TasksDatabase.Models.User>
@{
    ViewData["Title"] = "Пользователи";
}

<h2>Привелегии пользователей</h2>
<table class="table">
    @foreach (var user in Model)
    {
        <tr>
            <td>@user.UserName</td>
            <td>
                <a class="btn btn-sm btn-primary" asp-action="Edit" asp-route-userid="@user.Id">Права доступа</a>
            </td>
        </tr>
    }
</table>
<a class="btn btn-info" asp-action="Create">Добавить роль</a>
<a class="btn btn-info" asp-action="RolesList">Список ролей</a>